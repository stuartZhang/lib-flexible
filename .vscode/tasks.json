{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [{
      "type": "npm",
      "script": "build",
      "options": {
          "cwd": "${workspaceFolder}",
          "env": {}
      },
      "presentation": {
          "echo": true,
          "reveal": "always",
          "focus": false,
          "panel": "new"
      },
      "group": {
          "kind": "build",
          "isDefault": true
      },
      "isBackground": true,
      "problemMatcher": {
          "owner": "webpack",
          "background": {
              "activeOnStart": true,
              "beginsPattern": "Compiling\\.\\.\\.",
              "endsPattern": "DONE\\s+Compiled\\s+successfully\\s+in\\s+\\d+ms"
          },
          "pattern": [{
              "regexp": "^\\s*(\\S+)\\s*$",
              "file": 1
          }, {
              "regexp": "^\\s+(\\d+):(\\d+)\\s+(\\S+)\\s+(.*)\\s\\s+(.*)\\s*$",
              "line": 1,
              "column": 2,
              "severity": 3,
              "message": 4,
              "code": 5
          }]
      }
  }]
}